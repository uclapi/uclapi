{
  "name": "api-dashboard",
  "version": "1.0.2",
  "description": "Dashboard for the UCL API",
  "homepage": "https://uclapi.com",
  "bugs": {
    "url": "https://github.com/uclapi/uclapi/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/uclapi/uclapi.git"
  },
  "license": "ISC",
  "author": "UCL API",
  "scripts": {
    "analyse": "npx webpack-bundle-analyzer stats.json",
    "build": "cross-env NODE_ENV=production webpack --config webpack.prod.js --progress",
    "pre-commit": "lint-staged",
    "profile": "cross-env NODE_ENV=production webpack --config webpack.prod.js --progress --profile --json > stats.json",
    "start": "cross-env NODE_ENV=development webpack --config webpack.dev.js --watch --progress",
    "test": "jest",
    "test-generate": "jest --updateSnapshot"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run pre-commit"
    }
  },
  "lint-staged": {
    "*.{js,jsx,json}": [
      "eslint --fix"
    ],
    "*.{scss}": [
      "stylelint --fix"
    ],
    "../**/*.py": [
      "autopep8 --in-place --global-config ../backend/uclapi/.flake8.ini",
      "flake8 --config ../backend/uclapi/.flake8.ini"
    ]
  },
  "browserify": {
    "transform": [
      "babelify",
      "envify"
    ]
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "jsx"
    ],
    "moduleDirectories": [
      "node_modules",
      "bower_components",
      "shared"
    ],
    "modulePaths": [
      "/shared/vendor/modules"
    ],
    "moduleNameMapper": {
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/test/__mocks__/fileMock.js",
      "\\.(css|scss)$": "<rootDir>/test/__mocks__/styleMock.js",
      "^Images(.*)$": "<rootDir>/src/images$1",
      "^Layout(.*)$": "<rootDir>/src/components/layout$1",
      "^Dashboard(.*)$": "<rootDir>/src/components/dashboard$1",
      "^Styles(.*)$": "<rootDir>/src/sass$1"
    },
    "testEnvironment": "jsdom",
    "transform": {
      "\\.[jt]sx?$": "babel-jest"
    }
  },
  "dependencies": {
    "@material-ui/core": "^4.12.3",
    "@sentry/react": "^6.13.2",
    "@sentry/tracing": "^6.13.2",
    "axios": "^0.21.4",
    "dayjs": "^1.10.7",
    "framer-motion": "^4.1.17",
    "include-media": "^1.4.10",
    "js-cookie": "^3.0.1",
    "npm-check-updates": "^11.8.5",
    "prop-types": "^15.7.2",
    "qs": "^6.10.1",
    "rc-collapse": "^3.1.2",
    "react": "^17.0.2",
    "react-autosuggest": "^10.1.0",
    "react-dom": "^17.0.2",
    "react-icons": "^4.3.1",
    "react-modal": "^3.14.3",
    "react-responsive-carousel": "^3.2.22",
    "react-router-dom": "^5.3.0",
    "react-syntax-highlighter": "^15.4.4",
    "react-tabs": "^3.2.2",
    "swagger-ui-react": "^3.52.3",
    "whatwg-fetch": "^3.6.2"
  },
  "devDependencies": {
    "@babel/core": "^7.15.5",
    "@babel/eslint-parser": "^7.15.7",
    "@babel/plugin-proposal-class-properties": "^7.14.5",
    "@babel/plugin-syntax-dynamic-import": "^7.8.3",
    "@babel/plugin-transform-runtime": "^7.15.0",
    "@babel/preset-env": "^7.15.6",
    "@babel/preset-react": "^7.14.5",
    "@babel/runtime": "^7.15.4",
    "@typescript-eslint/parser": "^4.32.0",
    "babel-jest": "^27.3.1",
    "babel-loader": "^8.2.2",
    "babel-plugin-transform-imports": "^2.0.0",
    "babel-plugin-transform-react-remove-prop-types": "^0.4.24",
    "cross-env": "^7.0.3",
    "css-loader": "^6.3.0",
    "css-minimizer-webpack-plugin": "^3.0.2",
    "dotenv": "^10.0.0",
    "eslint": "^7.32.0",
    "eslint-import-resolver-alias": "^1.1.2",
    "eslint-import-resolver-webpack": "^0.13.2",
    "eslint-plugin-disable": "^2.0.1",
    "eslint-plugin-import": "^2.24.2",
    "eslint-plugin-jest": "^24.5.0",
    "eslint-plugin-json-format": "^2.0.1",
    "eslint-plugin-no-secrets": "^0.8.9",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-react": "^7.26.1",
    "eslint-plugin-react-hooks": "^4.2.0",
    "eslint-plugin-security": "^1.4.0",
    "eslint-plugin-sonarjs": "^0.10.0",
    "file-loader": "^6.2.0",
    "husky": "^7.0.2",
    "jest": "^27.2.4",
    "lint-staged": "^11.1.2",
    "mini-css-extract-plugin": "^2.3.0",
    "react-test-renderer": "^17.0.2",
    "sass": "^1.42.1",
    "sass-loader": "^12.1.0",
    "style-loader": "^3.3.1",
    "stylelint": "^13.13.1",
    "stylelint-config-sass-guidelines": "^8.0.0",
    "stylelint-config-standard": "^22.0.0",
    "stylelint-scss": "^3.21.0",
    "terser-webpack-plugin": "^5.2.4",
    "url-loader": "^4.1.1",
    "webpack": "^5.55.1",
    "webpack-bundle-tracker": "^1.4.0",
    "webpack-cli": "^4.8.0"
  }
}
